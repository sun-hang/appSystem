page {
  background-color: #fff;
}

.searchPage_search_container {
  margin: 20rpx;
}

.searchPage_history_container {
  width: 100%;
  margin: 120rpx 0 40rpx;
}

.searchPage_history_inner_container {
  width: 94%;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 10rpx 20rpx;
}

.searchPage_history_title_container {
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.searchPage_history_title_container mp-icon {
  cursor: pointer;
}

.searchPage_history_tags_container {
  display: flex;
  flex-wrap: wrap;
  margin-top: 15rpx;
}
.searchPage_history_tags_item{
  margin: 14rpx 15rpx;
  padding: 6rpx 30rpx;
  background-color: #F6F6F6;
  border-radius: 30rpx;
  font-size: .9rem;
  cursor: pointer;
}